page{ 
 height: 100%; 
 background-color: #272727;
}

.goods-list{
  display: flex;
  display: -webkit-flex;
  flex-direction:row ;
  justify-content: space-between;
  flex-wrap:wrap;
  padding-top: 3px;
  height: 400px;
  width: 100%;
}

.good{
  display: flex;
  display: -webkit-flex;
  flex-direction:column ;
  justify-content: space-between;
  align-items: center;
  padding: 7px 5px 7px 5px;
  width: 70px;
  height: 130px;
}

.good image{
  width: 50px;
  height: 50px;

}